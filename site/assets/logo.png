      .ring { fill:url(#ringGrad); }
      .ringBorder { fill:none; stroke:#b18436; stroke-width:3; }
      .star { fill:#ecd089; stroke:#a77930; stroke-width:2; }
      .m { fill:url(#goldBase); stroke:#7d571f; stroke-width:3; }
      .circuit { fill:none; stroke:url(#circuitGrad); stroke-width:3; stroke-linecap:round; }
      .pad { fill:#ead39b; stroke:#9b6f2d; stroke-width:2; }
